{
  "version": 2,
  "buildCommand": "cd zam_trans_app && flutter build web --release",
  "outputDirectory": "zam_trans_app/build/web",
  "installCommand": "cd zam_trans_app && flutter pub get",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
